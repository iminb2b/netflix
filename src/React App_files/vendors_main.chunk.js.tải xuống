async function getRecaptchaParams(auth) {
  return (await _performApiRequest(auth, "GET"
  /* GET */
  , "/v1/recaptchaParams"
  /* GET_RECAPTCHA_PARAM */
  )).recaptchaSiteKey || '';
}
