const gn = new fn(Rt.comparator);

function yn() {
  return gn;
}

const pn = new fn(Rt.comparator);

function En() {
  return pn;
}

const Tn = new fn(Rt.comparator);

function In() {
  return Tn;
}

const An = new _n(Rt.comparator);

function Rn(...t) {
  let e = An;

  for (const n of t) e = e.add(n);

  return e;
}

const bn = new _n(Z);

function Pn() {
  return bn;
}
/**
