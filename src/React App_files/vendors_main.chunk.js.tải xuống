/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";
// @remove-on-eject-begin

/**
 * Copyright (c) 2015-present, Facebook, Inc.
 *
 * This source code is licensed under the MIT license found in the
