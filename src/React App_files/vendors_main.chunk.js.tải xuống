async function deleteAccount(auth, request) {
  return _performApiRequest(auth, "POST"
  /* POST */
  , "/v1/accounts:delete"
  /* DELETE_ACCOUNT */
  , request);
}

async function deleteLinkedAccounts(auth, request) {
  return _performApiRequest(auth, "POST"
  /* POST */
  , "/v1/accounts:update"
  /* SET_ACCOUNT_INFO */
  , request);
}

async function getAccountInfo(auth, request) {
  return _performApiRequest(auth, "POST"
  /* POST */
  , "/v1/accounts:lookup"
  /* GET_ACCOUNT_INFO */
  , request);
}
