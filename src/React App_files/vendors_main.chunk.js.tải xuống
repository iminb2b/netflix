      }
    }),
          a = Ut.newFoundDocument(i, r, o),
          c = s.targetIds || [],
          u = s.removedTargetIds || [];
    n = new Sn(c, u, a.key, a);
  } else if ("documentDelete" in e) {
    e.documentDelete;
