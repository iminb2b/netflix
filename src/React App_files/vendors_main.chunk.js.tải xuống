function it(t) {
  let e = 0;

  for (const n in t) Object.prototype.hasOwnProperty.call(t, n) && e++;

  return e;
}

function rt(t, e) {
  for (const n in t) Object.prototype.hasOwnProperty.call(t, n) && e(n, t[n]);
}

function ot(t) {
  for (const e in t) if (Object.prototype.hasOwnProperty.call(t, e)) return !1;

  return !0;
}
