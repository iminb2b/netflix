function useAuthListener() {
  _s();

  const [user, setUser] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])(JSON.parse(localStorage.getItem("authUser")));
  Object(react__WEBPACK_IMPORTED_MODULE_0__["useEffect"])(() => {
    const auth = Object(firebase_auth__WEBPACK_IMPORTED_MODULE_1__["getAuth"])();
    const listener = Object(firebase_auth__WEBPACK_IMPORTED_MODULE_1__["onAuthStateChanged"])(auth, user => {
      if (user) {
        localStorage.setItem("authUser", JSON.stringify(user));
        setUser(user);
      } else {
        localStorage.removeItem("authUser");
        setUser(null);
      }
    });
